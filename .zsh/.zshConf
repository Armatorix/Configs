source ~/.gvm/scripts/gvm
source ~/.zsh/zsh-autosuggestions/zsh-autosuggestions.zsh

gvm use go1.8.3 1>/dev/null
export GOPATH="~/programming/go"
export GOINTEL="$GOPATH/src/githube.com/intelsdi-x"
export PATH=$HOME/.local/bin:$GOPATH/bin:$PATH

alias vimgo='vim -u ~/.vimrc.go'
alias myHttp='python -m SimpleHTTPServer 8080'

alias gcom='git commit -m'
alias gpush='git push origin'
alias gadd='git add'
alias gstat='git status'
alias gbran='git branch'
alias gclon='git clone'
gclx() {git clone git@github.com:Armatorix/$1.git;}
		
export TERM="xterm-256color"
